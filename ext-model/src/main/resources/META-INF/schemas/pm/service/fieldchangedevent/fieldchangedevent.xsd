<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<schema version="1.0" elementFormDefault="qualified"
	xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://dsp.ge.com/pm/solution/service/fieldchanged" 
	xmlns:fieldChanged="http://dsp.ge.com/pm/solution/service/entity/fieldchanged">

	<import schemaLocation="fieldchanged/fieldchanged.xsd"
		namespace="http://dsp.ge.com/pm/solution/service/entity/fieldchanged">
	</import>

	<!-- This is XSD for FieldChangedEvent which has FieldChangedList Object -->

	<import schemaLocation="fieldchanged/fieldchanged.xsd" namespace="http://dsp.ge.com/pm/solution/service/entity/fieldchanged"></import>
	<element name="fieldChangedEvent">
		<complexType>
			<sequence>
				<element ref="fieldChanged:fieldChangedList"></element>
			</sequence>
		</complexType>
	</element>

</schema>

